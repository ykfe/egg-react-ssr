{
  "name": "yk-cli",
  "version": "3.1.1",
  "description": "ykcli for ssr",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --sourceMap false\" \" tsc -p ./tsconfig.es6.json --sourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.es6.json \""
  },
  "bin": {
    "ykcli": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/yk-cli.git"
  },
  "dependencies": {
    "download-git-repo": "^3.0.2",
    "inquirer": "7.0.4",
    "ora": "^4.0.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "shelljs": "^0.8.3",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^6.0.3",
    "@types/shelljs": "^0.8.5",
    "@types/webpack": "^4.39.3",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/yargs": "^15.0.3",
    "concurrently": "^5.1.0",
    "webpack": "4.41.6",
    "webpack-dev-server": "3.10.3"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/ykfe/yk-cli/issues"
  },
