{% if language === "javascript" %}
{
  "name": "{{appName}}",
  "version": "2.0.0",
  "dependencies": {
    "egg": "^2.21.0",
    "egg-proxy": "^1.1.0",
    "egg-scripts": "^2.11.0",
    "egg-static": "^2.2.0",
